<ServerManagerConfiguration>
  <ProxyGroup name="array_measurement">
    <NullProxy name="None"></NullProxy>
    <Proxy class="vtkArithmeticMeanArrayMeasurement"
           name="Arithmetic Mean">
      <Hints>
        <ProxyList>
          <Link name="Input"
                with_property="Input" />
        </ProxyList>
      </Hints>
    </Proxy>
    <Proxy class="vtkGeometricMeanArrayMeasurement"
           name="Geometric Mean">
      <Hints>
        <ProxyList>
          <Link name="Input"
                with_property="Input" />
        </ProxyList>
      </Hints>
    </Proxy>
    <Proxy class="vtkHarmonicMeanArrayMeasurement"
           name="Harmonic Mean">
      <Hints>
        <ProxyList>
          <Link name="Input"
                with_property="Input" />
        </ProxyList>
      </Hints>
    </Proxy>
    <Proxy class="vtkMedianArrayMeasurement"
           name="Median">
      <Hints>
        <ProxyList>
          <Link name="Input"
                with_property="Input" />
        </ProxyList>
      </Hints>
    </Proxy>
    <Proxy class="vtkEntropyArrayMeasurement"
           name="Entropy">
      <Hints>
        <ProxyList>
          <Link name="Input"
                with_property="Input" />
        </ProxyList>
      </Hints>
    </Proxy>
    <Proxy class="vtkStandardDeviationArrayMeasurement"
           name="Standard Deviation">
      <Hints>
        <ProxyList>
          <Link name="Input"
                with_property="Input" />
        </ProxyList>
      </Hints>
    </Proxy>
  </ProxyGroup>
  <ProxyGroup name="filters" >
    <SourceProxy name="PVHyperTreeGridAnalysisDrivenRefinement" class="vtkPVHyperTreeGridAnalysisDrivenRefinement" label="Hyper Tree Grid - ADR" >
      <Documentation long_help="Adaptive Mesh Refinement using a Hyper Tree Grid."
                     short_help="Adaptive Mesh Refinement using a Hyper Tree Grid." >
          This Filter generates a Hyper Tree Grid using certain metrics to refine
          the input data set.
          As refinment goes, cells are box-clipped, which is relatively computationally expensive.
          The metric is a criteria such that if and only if the data set in the cell matches it, the tree is refined.
      </Documentation>
      <InputProperty name="Input"
                     command="SetInputConnection" >
            <ProxyGroupDomain name="groups" >
              <Group name="sources" />
              <Group name="filters" />
            </ProxyGroupDomain>
            <DataTypeDomain name="input_type" >
              <DataType value="vtkDataSet" />
            </DataTypeDomain>
      </InputProperty>
      <IntVectorProperty name="Dimensions"
                         label="Dimensions"
                         command="SetDimensions"
                         number_of_elements="3"
                         animateable="1"
                         default_values="3 3 3" >
          <Documentation>
            Dimensions of the Hyper Tree Grid. It should be 1 + a power of 2.
            The number of induced cells is a power of two.
          </Documentation>
      </IntVectorProperty>
      <IntVectorProperty name="BranchFactor"
                         label="Branch Factor"
                         command="SetBranchFactor"
                         number_of_elements="1"
                         default_values="2" >
          <EnumerationDomain name="enum" >
            <Entry value="2" text="2" />
            <Entry value="3" text="3" />
          </EnumerationDomain>
          <Documentation>
            Branch factor of the output Hyper Tree Grid.
          </Documentation>
      </IntVectorProperty>
      <IntVectorProperty name="MaxDepth"
                         label="MaxDepth"
                         command="SetMaxDepth"
                         number_of_elements="1"
                         animateable="0"
                         default_values="1" >
         <Documentation>
           Maximum tree depth of output Hyper Tree Grid
         </Documentation>
      </IntVectorProperty>
      <StringVectorProperty name="SelectInputScalars"
                            label="Array"
                            command="SetInputArrayToProcess"
                            number_of_elements="5"
                            element_types="int int int int str"
                            animateable="0" >
        <ArrayListDomain name="array_list"
                         attribute_type="Scalars"
                         input_domain_name="inputs_array" >
          <RequiredProperties>
            <Property name="Input"
                      function="Input" />
          </RequiredProperties>
        </ArrayListDomain>
      </StringVectorProperty>
      <IntVectorProperty name="UseMax"
                         command="SetMaxState"
                         number_of_elements="1"
                         default_values="1">
        <BooleanDomain name="bool" />
      </IntVectorProperty>
      <DoubleVectorProperty name="Max"
                            command="SetMax"
                            number_of_elements="1"
                            default_values="1">
        <Hints>
          <PropertyWidgetDecorator type="EnableWidgetDecorator">
            <Property name="UseMax" function="boolean" />
          </PropertyWidgetDecorator>
        </Hints>
      </DoubleVectorProperty>
      <IntVectorProperty name="UseMin"
                         command="SetMinState"
                         number_of_elements="1"
                         default_values="1">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <DoubleVectorProperty name="Min"
                            command="SetMin"
                            number_of_elements="1"
                            default_values="0" >
        <Hints>
          <PropertyWidgetDecorator type="EnableWidgetDecorator">
            <Property name="UseMin" function="boolean" />
          </PropertyWidgetDecorator>
        </Hints>
      </DoubleVectorProperty>
      <IntVectorProperty name="MinimumNumberOfPointsInSubtree"
                         command="SetMinimumNumberOfPointsInSubtree"
                         number_of_elements="1"
                         default_values="1" >
      </IntVectorProperty>
      <IntVectorProperty name="InsideRange"
                         command="SetInRange"
                         number_of_elements="1"
                         default_values="1" >
        <BooleanDomain name="bool" />
      </IntVectorProperty>
      <ProxyProperty command="SetArrayMeasurement"
                     label="Subdividing criterion"
                     name="ArrayMeasurement">
        <ProxyGroupDomain name="groups">
          <Group name="array_measurement" />
        </ProxyGroupDomain>
        <ProxyListDomain name="proxy_list">
          <Proxy group="array_measurement"
                 name="Arithmetic Mean" />
          <Proxy group="array_measurement"
                 name="Geometric Mean" />
          <Proxy group="array_measurement"
                 name="Harmonic Mean" />
          <Proxy group="array_measurement"
                 name="Median" />
          <Proxy group="array_measurement"
                 name="Standard Deviation" />
          <Proxy group="array_measurement"
                 name="Entropy" />
        </ProxyListDomain>
        <Documentation>This property sets the array measurement method used for
          subdividing leaves in the hyper tree grid.</Documentation>
      </ProxyProperty>
      <ProxyProperty command="SetArrayMeasurementDisplay"
                     label="Averaging method"
                     name="ArrayMeasurementDisplay">
        <ProxyGroupDomain name="groups">
          <Group name="array_measurement" />
        </ProxyGroupDomain>
        <ProxyListDomain name="proxy_list">
          <Proxy group="array_measurement"
                 name="None" />
          <Proxy group="array_measurement"
                 name="Arithmetic Mean" />
          <Proxy group="array_measurement"
                 name="Geometric Mean" />
          <Proxy group="array_measurement"
                 name="Harmonic Mean" />
          <Proxy group="array_measurement"
                 name="Median" />
          <Proxy group="array_measurement"
                 name="Standard Deviation" />
          <Proxy group="array_measurement"
                 name="Entropy" />
        </ProxyListDomain>
        <Documentation>This property sets an extra array measurement method only for visualization
          purposes.</Documentation>
      </ProxyProperty>
      <!-- End HyperTreeGridAnalysisDrivenRefinement -->
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
