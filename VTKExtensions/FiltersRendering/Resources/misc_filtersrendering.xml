<ServerManagerConfiguration>
  <ProxyGroup name="misc">
    <!-- ==================================================================== -->
    <SourceProxy class="vtkNetworkImageWriter" name="NetworkImageWriter" processes="client|dataserver">
      <IntVectorProperty name="OutputDestination"
                         command="SetOutputDestination"
                         number_of_elements="1"
                         default_values="0"
                         panel_visibility="never">
        <EnumerationDomain name="enum">
          <Entry value="0" text="CLIENT" />
          <Entry value="1" text="DATA_SERVER_ROOT" />
        </EnumerationDomain>
      </IntVectorProperty>
      <ProxyProperty name="Writer" command="SetWriter" />
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
