<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <!-- ==================================================================== -->
    <SourceProxy class="vtkNetworkImageSource"
                 name="NetworkImageSource"
                 post_push="UpdateImage"
                 processes="client|renderserver">
      <StringVectorProperty command="SetFileName"
                            name="FileName"
                            number_of_elements="1">
        <Documentation>Set the name of image file to load.</Documentation>
        <FileListDomain name="files" />
      </StringVectorProperty>
      <!-- End of NetworkImageSource -->
    </SourceProxy>

    <SourceProxy class="vtkNetworkImageData"
                 name="NetworkImageData"
                 post_push="UpdateBuffer"
                 processes="client|renderserver">
      <InputProperty command="SetInputConnection"
                     name="Input">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkImageData" />
        </DataTypeDomain>
      </InputProperty>
    <!-- End of NetworkImageData -->
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
