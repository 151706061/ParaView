#!/bin/sh

export PYTHONPATH="@catalyst_DIR@/../../../lib64/python3.6/site-packages":"@CMAKE_CURRENT_SOURCE_DIR@":$PYTHONPATH
export CATALYST_IMPLEMENTATION_PATHS="@ParaView_CATALYST_DIR@"
export CATALYST_IMPLEMENTATION_NAME=paraview

# we need to pass in the full path to the Python script we want to use since it's not picked up by looking
# in PYTHONPATH
"@pvbatch_exe@" --sym "@CMAKE_CURRENT_SOURCE_DIR@/fedriver.py" "@CMAKE_CURRENT_SOURCE_DIR@/../SampleScripts/gridwriter.py"
