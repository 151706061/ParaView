set kw(vtkTemp942) [$kw(vtkTemp1) InitializeReadCustom "MBP3DReader" "$DataDir/Data/comb_mb_ascii.mxyz"]
$kw(vtkTemp1) ReadFileInformation $kw(vtkTemp942) "$DataDir/Data/comb_mb_ascii.mxyz"
