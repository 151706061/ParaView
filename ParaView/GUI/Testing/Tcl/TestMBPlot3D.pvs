set kw(vtkTemp974) [$kw(vtkTemp942) GetPVWidget {Filename}]
$kw(vtkTemp974) SetValue "$DataDir/Data/comb_mb_ascii.mxyz"
set kw(vtkTemp943) [$kw(vtkTemp942) GetPVWidget {QFileName}]
$kw(vtkTemp943) SetValue "$DataDir/Data/comb_mb_ascii.mq"
set kw(vtkTemp952) [$kw(vtkTemp942) GetPVWidget {BinaryFile}]
$kw(vtkTemp952) SetSelectedState 0
set kw(vtkTemp955) [$kw(vtkTemp942) GetPVWidget {MultiGrid}]
$kw(vtkTemp955) SetSelectedState 1
set kw(vtkTemp958) [$kw(vtkTemp942) GetPVWidget {HasByteCount}]
$kw(vtkTemp958) SetSelectedState 0
set kw(vtkTemp961) [$kw(vtkTemp942) GetPVWidget {IBlanking}]
$kw(vtkTemp961) SetSelectedState 0
set kw(vtkTemp964) [$kw(vtkTemp942) GetPVWidget {TwoDimensionalGeometry}]
$kw(vtkTemp964) SetSelectedState 0
set kw(vtkTemp967) [$kw(vtkTemp942) GetPVWidget {ForceRead}]
$kw(vtkTemp967) SetSelectedState 0
set kw(vtkTemp970) [$kw(vtkTemp942) GetPVWidget {ByteOrder}]
$kw(vtkTemp970) SetCurrentValue {0}
$kw(vtkTemp942) AcceptCallback

set kw(vtkTemp1076) [$kw(vtkTemp1) CreatePVSource Cut]
$kw(vtkTemp1076) SetLabel {Cut0}
set kw(vtkTemp1077) [$kw(vtkTemp1076) GetPVWidget {Input}]
$kw(vtkTemp1077) SetCurrentValue $kw(vtkTemp942)
set kw(vtkTemp1120) [$kw(vtkTemp1076) GetPVWidget {Cut Function}]
$kw(vtkTemp1120) SetCurrentValue {Plane}
set kw(vtkTemp1120) [$kw(vtkTemp1076) GetPVWidget {Cut Function}]
set kw(vtkTemp1088) [$kw(vtkTemp1120) GetPVWidget {Plane}]
$kw(vtkTemp1088) SetCenter 8.255 0 30.831
$kw(vtkTemp1088) SetNormal 0 0 1
set kw(vtkTemp1121) [$kw(vtkTemp1076) GetPVWidget {Cut Values}]
$kw(vtkTemp1121) RemoveAllValues
