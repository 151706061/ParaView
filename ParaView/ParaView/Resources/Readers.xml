<ModuleInterfaces>
 <Module name="PVDReader" class="vtkPVDReaderModule"
         root_name="PVDReader" module_type="Reader"
         extensions=".pvd" file_description="ParaView Data Files">
  <Source class="vtkXMLCollectionReader"/>
 </Module>
 <Module name="XMLPolyDataReader"
         root_name="XMLPolyDataReader" 
         class="vtkPVAdvancedReaderModule"
         module_type="Reader"
         extensions=".vtp"
         file_description="VTK PolyData Files">
  <Source class="vtkXMLPolyDataReader"/>
  <ArraySelection attribute_name="Point" />
  <ArraySelection attribute_name="Cell" />
 </Module>
 <Module name="XMLUnstructuredGridReader"
