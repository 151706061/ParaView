$Application ExitOnReturnOn

set kw(vtkTemp4) [$Application GetMainWindow]
set kw(vtkTemp18) [$kw(vtkTemp4) GetMainView]

if {[info exists argc]} { 
    set argcm1 [expr $argc - 1]
    for {set i 0} {$i < $argcm1} {incr i} {
        if {[lindex $argv $i] == "-D" && $i < $argcm1} {
		set DataDir [lindex $argv [expr $i + 1]]
        }
        if {[lindex $argv $i] == "-UC" && $i < $argcm1} {
            set rmui [$kw(vtkTemp18) GetRenderModuleUI]
            catch {$rmui SetCompositeThreshold 0}
        }
    }
} 

