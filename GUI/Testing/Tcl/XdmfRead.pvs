set kw(vtkTemp2) [$Application GetMainWindow]
set kw(vtkTemp18) [$kw(vtkTemp2) GetMainView]

source [file join [file dirname [info script]] CommandLineOptions.tcl]
ParseCommandLine $kw(vtkTemp18) $argv $argc

puts "${DataDir}/Data/Test1.xmf"

$kw(vtkTemp18) SetOrientationAxesVisibility 0
set kw(vtkTemp582) [$kw(vtkTemp2) InitializeReadCustom "XdmfReader" "${DataDir}/Data/Test1.xmf"]
$kw(vtkTemp582) SetDomain {Domain0}
$kw(vtkTemp582) EnableGrid {Unnamed}
$kw(vtkTemp2) ReadFileInformation $kw(vtkTemp582) "${DataDir}/Data/Test1.xmf"
$kw(vtkTemp2) FinalizeRead $kw(vtkTemp582) "${DataDir}/Data/Test1.xmf"
set kw(vtkTemp606) [$kw(vtkTemp582) GetPVWidget {Filename}]
$kw(vtkTemp606) SetValue "${DataDir}/Data/Test1.xmf"
$kw(vtkTemp582) AcceptCallback
$kw(vtkTemp18) SetCameraState 10.265 5.720 -1.159  1.500 1.000 -1.000  -0.406 0.735 -0.544

$kw(vtkTemp18) SetCameraState -0.450 7.144 -8.589  1.500 1.000 -1.000  -0.968 0.003 0.251
$kw(vtkTemp2) ResetCameraCallback
set kw(vtkTemp1094) [$kw(vtkTemp2) CreatePVSource Glyph]
set kw(vtkTemp1120) [$kw(vtkTemp1094) GetPVWidget {Glyph}]
set kw(vtkTemp178) [$kw(vtkTemp2) GetPVSource GlyphSources Arrow0]
$kw(vtkTemp1120) SetCurrentValue $kw(vtkTemp178)
set kw(vtkTemp1107) [$kw(vtkTemp1094) GetPVWidget {OrientScale}]
$kw(vtkTemp1107) SetOrientMode {Vector}
$kw(vtkTemp1107) SetVectors {}
$kw(vtkTemp1107) SetScaleMode {Scalar}
$kw(vtkTemp1107) SetScalars {DataTwo}
$kw(vtkTemp1094) AcceptCallback
$kw(vtkTemp1094) DeleteCallback
$kw(vtkTemp2) SetCurrentPVSourceCallback $kw(vtkTemp582)
$kw(vtkTemp18) SetCameraState 9.087 1.279 -1.442  1.162 1.000 -1.000  -0.021 -0.629 -0.777
$kw(vtkTemp18) SetCameraState 7.797 -2.031 2.139  1.162 1.000 -1.000  0.112 -0.586 -0.802
$kw(vtkTemp18) SetCameraState 1.624 -5.207 -5.932  1.162 1.000 -1.000  0.098 0.624 -0.776



set kw(vtkTemp1346) [$kw(vtkTemp2) CreatePVSource ShrinkFilter]
$kw(vtkTemp1346) AcceptCallback

[$kw(vtkTemp1346) GetPVOutput] SetInterpolateColorsFlag 1

$kw(vtkTemp18) SetCameraState 9.004 1.696 -2.043  1.162 1.000 -1.000  -0.143 0.850 -0.508
$kw(vtkTemp18) SetCameraState 0.597 -1.103 -8.637  1.162 1.000 -1.000  -0.467 0.861 -0.202
$kw(vtkTemp2) ResetCameraCallback

# Fix the size of the image.
set MainView [[$Application GetMainWindow] GetMainView]
$MainView SetRenderWindowSize 300 300
update
[[$Application GetMainWindow] GetMainView] ForceRender

set didRegression 0

for {set i  1} {$i < [expr $argc - 1]} {incr i} {
    if {[lindex $argv $i] == "-C"} {
	source [lindex $argv [expr $i + 1]]
	$Application SetExitStatus [CompareImage [[$Application GetMainWindow] GetMainView]]
       set didRegression 1
    }
}

if { ! $didRegression } {

    set colorMaps [[$Application GetMainWindow] GetPVColorMaps]
    $colorMaps InitTraversal
    while { 1 } {
        set colorMap [$colorMaps GetNextItemAsObject]
        if { $colorMap == {} } {
            break
        }
        $colorMap SetScalarBarVisibility 0
    }
    for {set i  1} {$i < [expr $argc - 1]} {incr i} {
        if {[lindex $argv $i] == "-BT"} {
            source [lindex $argv [expr $i + 1]]
        }
    }
}

#source [file join [file dirname [info script]] Compare.tcl]
#Compare $Application $argv $argc
   
$Application Exit
